{{# def.definitions }}
{{# def.errors }}
{{# def.setup:'allOf' }}
{{# def.setupNextLevel }}

{{~ $schema:$sch:$i }}
  {{? {{# def.nonEmptySchema:$sch }} }}
    {{
      $it.schema = $sch;
      $it.schemaPath = $schemaPath + '[' + $i + ']';
    }}

    {{= it.validate($it) }}

    {{# def.ifResultValid }}
  {{?}}
{{~}}

{{? $breakOnError }}
  {{= $closingBraces.slice(0,-1) }}
{{?}}

{{# def.cleanUp }}
